<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.cny.yurayura.dao.sys.dict.DictMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.cny.yurayura.entity.sys.dict.Dict">
        <id column="id" property="id"/>
        <result column="dict_code" property="dictCode"/>
        <result column="dict_name" property="dictName"/>
        <result column="dict_val" property="dictVal"/>
        <result column="dict_status" property="dictStatus"/>
        <result column="dict_seq" property="dictSeq"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, dict_code, dict_name, dict_val, dict_status, dict_seq
    </sql>

</mapper>
